	.email-subscribe h1{
		display: none;
	}

	#subscribe-email input{
		width: 100%;
		background: #f0f5e7;
		border: 1px solid #cde0d8;
		color: #4f7566;
		border-radius: 0;
	}

	#subscribe-submit input[type="submit"]{
		width: 100%;
		background: #cde0d8;
		border: 1px solid #cde0d8;
		color: #4f7566;
		border-radius: 0;
		text-transform: uppercase;
		font-weight: bold;
		font-size: 14px;
	}

	.site-tagline{
		display: inline-block;
		float: right;
		clear: right;
		width: 240px;
		font-style: italic;
		padding: 20px;
		text-align: center;
		background-image: url('images/gradent2.png');
		background-size: 270px 97px;
		background-repeat: no-repeat;

	}

	#site-navigation{
		position: static;
		width: 100%;
		padding: 0 30px;

		-webkit-box-shadow: 0px 4px 10px 0px rgba(177, 194, 187, 1);
		-moz-box-shadow:    0px 4px 10px 0px rgba(177, 194, 187, 1);
		box-shadow:         0px 4px 10px 0px rgba(177, 194, 187, 1);
	}

	#site-navigation li{
		width: auto;
		border-bottom: none;
	}

	#site-navigation a{
		display: inline-block;
		padding: 20px 10px;
	}

	#site-navigation a:hover, a:active{
		background: #fff;
}

	/*---------------------------------------
	CONTENT
	---------------------------------------*/

	.col-1{
		float: right;
		width: 60%;
		padding-left: 40px;
	}

	.col-2{
		float: right;
		width: 40%;
	}

	.col-2 iframe{
		margin-top: 0;
		width: 100%;
	}

	.col-3{
		clear: both;
	}

	.right-section{
		float: left;
		width: 50%;
		padding-left: 20px;
	}

	.right-section-expand{
		clear: both;
	}

	.left-section{
		float: left;
		width: 50%;
	}

	/*---------------------------------------
	FOOTER
	---------------------------------------*/
	#contact{
		float: left;
	}

	#copyright-paragraph{
		display: none;
	}

	#copyright-single{
		display: block;
		float: right;
		margin-top: 30px;
	}

}

/*--------------------------------------------------------------
LARGE VIEWPORT
--------------------------------------------------------------*/
@media screen and (min-width: 1060px) {
	/*---------------------------------------
	GENERAL
	---------------------------------------*/
	.container{
		width: 960px;
		margin: 0px auto;
	}

	.container-desktop-nav{
		width: 1020px;
		margin: 0px auto;
	}

	/*---------------------------------------
	HEADER
	---------------------------------------*/
	.header-right{
		padding-right: 0;
	}

	.site-branding img{
		width: 50%;
		padding: 20px 0;
	}

	.site-tagline{
		margin-top: 50px;
		width: 570px;
		background-image: url('images/gradent2.png');
		background-size: 590px 70px;
		background-repeat: no-repeat;
	}

	#site-navigation a{
		padding: 15px 37px;
	}


	/*---------------------------------------
	CONTENT
	---------------------------------------*/
	.promo-links a{
		width: 290px;
		float: left;
	}

	.promo-links a:nth-child(2){
		margin: 35px 45px;
	}

	.small-link-image{
		display: none;
	}

	.large-link-image{
		display: block;
		/*height: 175px;*/
	}

	.col-1{
		float: left;
		max-width: 290px;
		margin: 0;
		padding: 0;
	}

	.col-2{
		float: left;
		max-width: 290px;
		margin: 0 45px;
	}

	.col-3{
		clear: none;
		float: left;
		max-width: 290px;
	}

	.right-section-expand{
		clear: none;
		float: left;
		width: 50%;
		padding-left: 20px;
	}

	/*---------------------------------------
	FOOTER
	---------------------------------------*/
	#footer-content{
		padding: 25px 0;
	}
}